TITLE EXIT V.30.0.160
SUBTTL	20-MAY-71	/DMN

ENTRY	EXIT,EXIT.,EXIT0,EXITP.
EXTERN	ERRFIN,UUOL.
OPDEF	RESET.	[15B8]

P=17		;PUSH DOWN POINTER


	SIXBIT /EXIT0/
EXIT0:	0
	JRST	EXITA		;DON'T GO TO ERRFIN

	SIXBIT /EXIT/
EXIT:	0
EXIT.:	XCT	UUOL.		;SET UP AC 16
EXITP.:	PUSHJ	P,ERRFIN	;PRINT OUT ERROR SUMMARY
	XCT	UUOL.		;RESTORE AC16 FOR USER
EXITA:	RESET.			;CLOSE I/O AND CLEAR TABLES
	CALLI	12
END

