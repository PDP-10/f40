	TITLE	AIMAG  V.022
	SUBTTL	29-NOVEMBER-1969	ED YOURDON/KK

;FROM V.005
;COMPLEX TO IMAGINARY CONVERSION FUNCTION
;THIS ROUTINE RETURNS THE IMAGINARY PART OF A COMPLEX NUMBER

;THE ROUTINE IS CALLED IN THE FOLLOWING MANNER:
;	JSA	Q, AIMAG
;	EXP	ARG
;THE ANSWER IS RETURNED IN ACCUMULATOR A

	ENTRY	AIMAG
	A=	0
	B=	1
	Q=	16

	SIXBIT/AIMAG/
AIMAG:	0			;ENTRY TO AIMAG ROUTINE
	MOVE	B,(Q)		;PUT IMAG(ARG)
	MOVE	A,1(B)		;IN AC A.
	JRA	Q, 1(Q)		;EXIT

	END

